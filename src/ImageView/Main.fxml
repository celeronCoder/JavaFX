<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="607.0" prefWidth="674.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ImageView.Controller">
   <children>
      <Button fx:id="switchBtn" layoutX="284.0" layoutY="524.0" mnemonicParsing="false" onAction="#toggleImage" text="Switch Image" />
      <ImageView fx:id="imageContainer" fitHeight="464.0" fitWidth="552.0" layoutX="182.0" layoutY="14.0" opacity="0.89" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@building.jpg" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
</AnchorPane>
